{
  "name": "frontium-videos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache",
    "reinstall": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "stop-dev": "powershell -Command \"Get-Process -Name node | Where-Object { $_.ProcessName -like '*next*' -or $_.CommandLine -like '*next*' } | Stop-Process -Force\"",
    "stop-dev-bash": "cmd.exe /C taskkill /F /IM node.exe 2>/dev/null || echo 'No Node.js processes found'",
    "kill-port": "powershell -Command \"Get-NetTCPConnection -LocalPort 3000 | ForEach-Object { Stop-Process -Id $_.OwningProcess -Force }\"",
    "kill-port-bash": "npx kill-port 3000 2>/dev/null || echo 'Port 3000 is free'"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.25.5",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.524.0",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}